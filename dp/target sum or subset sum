public class Main {
    public static void main(String[] args) {
        int[] arr={2,3,5,8,10};
        int n = arr.length;
        int target = 11;
       for(int i=0; i<n; i++){
        for(int j=i+1; j<n; j++){
           if(arr[i]+arr[j]==target){
                System.out.println("pair ("+arr[i]+","+arr[j]+")");
           } 
         }  
       }
    }
}
